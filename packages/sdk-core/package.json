{
  "name": "@actionpackd/sdk-core",
  "version": "0.1.0",
  "description": "Actionpackd AI SDK - Secure, typed AI interactions with multiple providers",
  "repository": {
    "type": "git",
    "url": "https://github.com/actionpackd/ai-sdk.git"
  },
  "keywords": [
    "ai",
    "sdk",
    "typescript",
    "openai",
    "anthropic",
    "gemini",
    "streaming",
    "type-safe",
    "security"
  ],
  "author": "Actionpackd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actionpackd/ai-sdk/issues"
  },
  "homepage": "https://github.com/actionpackd/ai-sdk#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.cjs",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --config ../../vitest.config.ts",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.18.0",
    "@types/react": "^18.3.24",
    "react": "^18.2.0",
    "tsup": "^8.3.5",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
